<Window x:Class="PROJEKTLoty.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PROJEKTLoty"
        mc:Ignorable="d"
        Title="MainWindow" Height="1024" Width="1280">
    <Grid Name="MainGrid" Height="Auto" Width="Auto">
        <Frame x:Name="Left" HorizontalAlignment="Left" Height="1000" VerticalAlignment="Top" Width="1000" Navigated="Left_Navigated" Background="Black"/>
        <Frame x:Name="Right"  HorizontalAlignment="Right" Height="1000" VerticalAlignment="Top" Width="280" Navigated="Right_Navigated" Background="LightBlue"/>
        <Button x:Name="Start" Content="Start" HorizontalAlignment="Center" Margin="1077,66,0,0" VerticalAlignment="Top" Width="147" Height="99" Click="Start_Click"/>
        <Button x:Name="Stop" Content="Stop" HorizontalAlignment="Left" Margin="1098,242,0,0" VerticalAlignment="Top" Width="148" Height="120" Click="Stop_Click"/>



    </Grid>
</Window>
