        <Button x:Name="Stop" Content="Stop" HorizontalAlignment="Left" Margin="1048,148,0,0" VerticalAlignment="Top" Width="148" Height="120" Click="Stop_Click"/>
        <TextBlock x:Name="wynik" HorizontalAlignment="Left" Margin="1014,608,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Height="289" Width="246"/>
